{
  "name": "@uniswap/narwhal-sdk",
  "version": "1.0.0-beta.9",
  "description": "sdk for integrating with the narwhal contracts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "build": "tsdx build",
    "prettier:fix": "prettier --write '**/*.ts' && prettier --write '**/*.json'",
    "prettier": "prettier --check '**/*.ts' && prettier --check '**/*.json'",
    "docs": "typedoc"
  },
  "author": "@Uniswap",
  "peerDependencies": {},
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.2",
    "chai": "^4.3.4",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.0",
    "prettier": "^2.3.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.3"
  },
  "dependencies": {
    "@uniswap/narwhal": "../narwhal",
    "@uniswap/router-sdk": "^1.4.0",
    "@uniswap/sdk-core": "^3.1.0",
    "@uniswap/v2-sdk": "^3.0.1",
    "@uniswap/v3-sdk": "^3.9.0",
    "bignumber.js": "^9.0.2",
    "ethers": "^5.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Uniswap/narwhal-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Uniswap/narwhal-sdk/issues"
  },
  "homepage": "https://github.com/Uniswap/narwhal-sdk#readme"
}
