{
  "name": "@uniswap/narwhal-sdk",
  "version": "1.0.0-beta.11",
  "description": "Fork of @weiroll/weiroll.js package to accomodate the narwhal contracts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/planner.test.ts' 'tests/routerCommands.test.ts'",
    "start": "tsdx watch",
    "build": "tsdx build",
    "tsdx-test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --write \"./src/*.{js,ts}\" \"tests/*.{js,ts}\"",
    "docs": "typedoc"
  },
  "author": "@Uniswap",
  "module": "dist/weiroll.js.esm.js",
  "license": "MIT",
  "peerDependencies": {},
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/weiroll.js.cjs.production.min.js",
      "limit": "35 KB"
    },
    {
      "path": "dist/weiroll.js.esm.js",
      "limit": "35 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.12.0",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.2",
    "chai": "^4.3.4",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.0",
    "prettier": "^2.3.1",
    "size-limit": "^4.12.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.3"
  },
  "dependencies": {
    "ethers": "^5.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Uniswap/narwhal-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Uniswap/narwhal-sdk/issues"
  },
  "homepage": "https://github.com/Uniswap/narwhal-sdk#readme"
}
